<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.7.1</version>
	
	</parent>
	<groupId>com.dunkware.trade.service</groupId>
	<artifactId>dunkware-trade-service-stream-server</artifactId>
	<version>1.3.0-SNAPSHOT</version>
	<name>dunkware-trade-service-stream-server</name>
	<description>Dunkware Trade Stream Service Server</description>


	<properties>
		<java.version>11</java.version>
		<org.mapstruct.version>1.4.2.Final</org.mapstruct.version>
		<flyway.version>7.10.0</flyway.version>
		<lombok.version>1.18.20</lombok.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<protoc.version>3.6.1</protoc.version>
		<protobuf.plugin.version>0.6.1</protobuf.plugin.version>
		<net-devh-grpc.version>2.0.1.RELEASE</net-devh-grpc.version>
		<grpc.version>1.32.1</grpc.version>
		
	</properties>

	<repositories>
		<repository>
			<id>dunkware-internal-release</id>
			<name>Dunkware Internal Release</name>
			<url>https://nexus.dunkware.com/repository/dunkware-internal-release/</url>
		</repository>
		<repository>
			<id>central</id>
			<name>Maven Central</name>
			<url>https://repo1.maven.org/maven2/</url>
		</repository>
		<repository>
			<id>dunkware-internal-snapshot</id>
			<name>Dunkware Internal Snapshot</name>
			<url>https://nexus.dunkware.com/repository/dunkware-internal-snapshot/</url>
		</repository>
	</repositories>


	<!-- Distribution Repos -->
	<distributionManagement>
		<snapshotRepository>
			<uniqueVersion>false</uniqueVersion>
			<id>dunkware-internal-snapshot</id>
			<layout>default</layout>
			<name>dunkware-internal-snapshot</name>
			<url>https://nexus.dunkware.com/repository/dunkware-internal-snapshot/</url>
		</snapshotRepository>
		<repository>
			<uniqueVersion>false</uniqueVersion>
			<id>dunkware-internal-release</id>
			<layout>default</layout>
			<name>dunkware-internal-release</name>
			<url>http://nexus.dunkware.com/repository/dunkware-internal-release/</url>
		</repository>
	</distributionManagement>

	<dependencies>

		<!-- https://mvnrepository.com/artifact/org.springframework.boot/spring-boot-starter-data-mongodb -->
