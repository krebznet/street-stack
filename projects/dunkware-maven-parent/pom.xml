<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.dunkware.maven</groupId>
	<artifactId>dunkware-maven-parent</artifactId>
	<version>6-SNAPSHOT</version>
	<name>Dunkware Maven Parent</name>
	<description>Dunkware Company Parent Pom</description>
	<packaging>pom</packaging>


	<scm>
		<connection>
			scm:git:ssh://git@github.com/krebznet/dunkware-maven-parent.git</connection>
		<developerConnection>
			scm:git:ssh://git@github.com/krebznet/dunkware-maven-parent.git</developerConnection>
		<url>https://github.com/krebznet/dunkware-maven-parent.git</url>
		<tag>v1</tag>
	</scm>


	<repositories>
		<repository>
			<id>dunkware-internal-release</id>
			<name>Dunkware Internal Release</name>
			<url>
				https://nexus.dunkware.com/repository/dunkware-internal-release/</url>
		</repository>
		<repository>
			<id>central</id>
			<name>Maven Central</name>
			<url>https://repo1.maven.org/maven2/</url>
		</repository>
		<repository>
			<id>dunkware-internal-snapshot</id>
			<name>Dunkware Internal Snapshot</name>
			<url>
				https://nexus.dunkware.com/repository/dunkware-internal-snapshot/</url>
		</repository>
	</repositories>


	<distributionManagement>
		<snapshotRepository>
			<uniqueVersion>false</uniqueVersion>
			<id>dunkware-internal-snapshot</id>
			<layout>default</layout>
			<name>dunkware-internal-snapshot</name>
			<url>
				https://nexus.dunkware.com/repository/dunkware-internal-snapshot/</url>

		</snapshotRepository>
		<repository>
			<uniqueVersion>false</uniqueVersion>
			<id>dunkware-internal-release</id>
			<layout>default</layout>
			<name>dunkware-internal-release</name>
			<url>
				https://nexus.dunkware.com/repository/dunkware-internal-release/</url>

		</repository>
	</distributionManagement>

	<properties>
		<test.dir>src/test/java</test.dir>
		<testSourceDirectory>/src/test/java</testSourceDirectory>
		<project.build.testSourceDirectory>src/test/java</project.build.testSourceDirectory>

		<src.dir>src/main/java</src.dir>
		<java.version>17</java.version>
		<maven.compiler.target>17</maven.compiler.target>
		<maven.compiler.source>17</maven.compiler.source>

		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<!-- Dependency Versions -->
		<logback.classic-version>1.2.3</logback.classic-version>
		<slf4j-api-version>1.7.30</slf4j-api-version>
		<junit-version>4.11</junit-version>
	</properties>




</project>
