		<label>logging</label>
		<remoteHost>172.16.16.55</remoteHost>
		<port>32756</port>
		<additionalField>
			<key>owner</key>
			<value>data-dev</value>
		</additionalField>
	</appender>-->
	

	

	<root level="INFO">
		<appender-ref ref="CONSOLE" additivity="false" />
	</root>

	
	<logger name="com.dunkware" level="DEBUG" additivity="false">
		
		<appender-ref ref="CONSOLE" additivity="false" />
	
		<!--  <appender-ref ref="FLUENT" additivity="false" />-->
	</logger>
	
	





</configuration>
